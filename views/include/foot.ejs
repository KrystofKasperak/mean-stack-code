<script type="text/javascript">
    /* Pop Up START */
    $(function() {
     $('a[rel*=leanModal]').leanModal({ top : 200, closeButton: ".modal_close" });
    });
    /* Pop Up END */

   /* Custom Select Javascript START */
    function DropDown(el) {
        this.dd = el;
        this.initEvents();
    }
    DropDown.prototype = {
        initEvents : function() {
            var obj = this;

            obj.dd.on('click', function(event){
                $(this).toggleClass('active');
                event.stopPropagation();
            });
        }
    }

    $(function() {

        var dd = new DropDown( $('#dd') );

        $(document).click(function() {
            // all dropdowns
            $('.wrapper-dropdown-5').removeClass('active');
        });

    });
    /* Custom Select Javascript END */

    /* Table theme Javascript START */
    $("#mytable #checkall").click(function () {
        if ($("#mytable #checkall").is(':checked')) {
            $("#mytable input[type=checkbox]").each(function () {
                $(this).prop("checked", true);
            });

        } else {
            $("#mytable input[type=checkbox]").each(function () {
                $(this).prop("checked", false);
            });
        }
    });

    $("[data-toggle=tooltip]").tooltip();
    /* Table theme Javascript END */

    /* Add active class to menu START */
    var currentURL = $(location).attr('href');
    var seg = currentURL.split('/');
    var active_class = seg;
    /* Add active class to menu END */

    /* MENU toggle action START */
    $('.nav-toggle').click(function(){
        //get collapse content selector
        var collapse_content_selector = $(this).attr('id');
        //make the collapse content to be shown or hide
        $(collapse_content_selector).toggle();
    });
    /* MENU toggle action END */
</script>
